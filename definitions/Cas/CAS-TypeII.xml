<model inter_gene_max_space="5" min_mandatory_genes_required="1" min_genes_required="1" vers="2.0">

#Present in different CRISPR-Cas systems
<gene name="Cas1_0_I-II-III" presence="accessory"/>
<gene name="Cas1_0_I-II-III-V" presence="accessory"/>

<gene name="Cas2_0_I-II-III" presence="accessory"/>
<gene name="Cas2_0_I-II-III-V" presence="accessory"/>

<gene name="Cas6_0_I-III" presence="accessory"/>
<gene name="Cas4_0_I-II" presence="accessory"/>

#Specific to Type II
<gene name="Cas1_0_II" presence="accessory"/>
<gene name="Cas9_0_II" presence="mandatory"/>
<gene name="Cas9_0_IIB" presence="mandatory"/>
<gene name="Csn2_0_IIA" presence="mandatory"/>
<gene name="Cas9_1_IIB" presence="mandatory"/>
<gene name="Cas9_1_II" presence="mandatory"/>

#Associated with Type II
<gene name="Cas3_0_I" presence="accessory"/>
<gene name="Cas3_1_I" presence="accessory"/>


#Distinguishing  from other Types
<gene name="Cas10_0_IIIA" presence="forbidden"/>
<gene name="Cas10_0_IIIB" presence="forbidden"/>





</model>

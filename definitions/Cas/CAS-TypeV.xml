<model inter_gene_max_space="5" min_mandatory_genes_required="1" min_genes_required="1" vers="2.0">

#Present in other CRISPR-Cas system
<gene name="Cas2_0_I-II-III" presence="accessory"/>
<gene name="Cas2_0_I-II-III-V" presence="accessory"/>
<gene name="Cas1_0_I-II-III-V" presence="accessory"/>
<gene name="Cas1_0_I-II-III" presence="accessory"/>

#Specific to Type IV
<gene name="Cas1_0_V" presence="accessory"/>
<gene name="Cas4_0_V" presence="mandatory"/>
<gene name="Cas4_0_I-II-V" presence="accessory"/>
<gene name="Cas12a_0_VA" presence="mandatory"/>
<gene name="Cas12a_1_VA" presence="mandatory"/>

<gene name="Cas12b_0_VB" presence="mandatory"/>
<gene name="Cas12c_0_VC" presence="mandatory"/>



#Distinguishing from other Types
<gene name="Cas3_0_I" presence="forbidden"/>
<gene name="Cas3_1_I" presence="forbidden"/>
<gene name="Cas9_0_II" presence="forbidden"/>
<gene name="Cas9_0_IIB" presence="forbidden"/>
<gene name="Cas10_0_IIIA" presence="forbidden"/>
<gene name="Cas10_0_IIIB" presence="forbidden"/>

</model>
